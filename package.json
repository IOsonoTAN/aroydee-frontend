{
  "name": "seven-cms",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "cloudinary": "^1.10.0",
    "cloudinary-react": "^1.0.6",
    "eslint": "^4.12.1",
    "eslint-plugin-react": "^7.5.1",
    "material-ui": "^0.20.0",
    "material-ui-next": "npm:material-ui@next",
    "material-ui-pagination": "^1.1.6",
    "moment": "^2.20.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.8",
    "react-facebook-login": "git+https://git@github.com/keppelen/react-facebook-login.git",
    "react-google-login": "^3.0.2",
    "react-paginate": "^5.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-swipeable-views": "^0.12.13",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.4.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "dev": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "babel": {
    "plugins": [
      "transform-decorators-legacy"
    ]
  }
}
